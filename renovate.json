{
  "customManagers": [
    {
      "fileMatch": ["^wpp-connect-addon/config\\.yaml$"],
      "datasource": "github-releases",
      "depName": "wppconnect-team/wppconnect-server",
      "extractVersion": "version:\\s+\"(?<currentValue>.*?)\"",
      "extractVersionTemplate": "{{{replace '^v' '' currentValue}}}",
      "versioningTemplate": "semver",
      "updateStrategy": "replace"
    }
  ]
}
